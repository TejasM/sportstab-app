package com.example.coachingtab;

public class MatrixData implements java.io.Serializable{

	private float[] data;
	
	public MatrixData(){
		data = new float[9];
	}
	
	public void setMatrixData(float[] input){
		for (int i = 0; i < 9; i++){
			data[i] = input[i];
		}
	}
	
	public void getMatrixData(float[] input){
		for (int i = 0; i < 9; i++){
			input[i] = data[i];
		}
	}
}
